
spring.datasource.initialization-mode=always
spring.datasource.platform=postgres
spring.jpa.show-sql=true
spring.jpa.hibernate.ddl-auto=create
# Add The name of your postgres database at the end of the url below
spring.datasource.url=jdbc:postgresql://localhost:5432/facespace
# Add below properties if a username and password are used to open your 
posgtres shell
spring.datasource.username=postgres
spring.datasource.password=06272015
